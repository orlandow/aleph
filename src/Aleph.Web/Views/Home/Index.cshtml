@model IEnumerable<Aleph.Web.Models.Card>
@{
    ViewBag.Title = "Hello";
}

<div style="width:500px;">
    <img src="@Url.Action("Get", "Images", new { width = 50, height = 50 })" />
    <ul class="list-group">
        @foreach (var item in Model)
        {
            @:@Html.Partial("_Card", item)
        }
    </ul>
</div>